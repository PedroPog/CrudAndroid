<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BtnWelcome">
        <item name="android:background">@drawable/btn_wellcome</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">25dp</item>
    </style>
    <style name="BtnConfirm">
        <item name="android:background">@drawable/btn_confirm</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="BtnConfirmBlue">
        <item name="android:background">@drawable/btn_confirm_blue</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="BtnComada">
        <item name="android:background">@drawable/btn_comanda</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="BtnInformarCpf">
        <item name="android:background">@drawable/btn_confirm</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="MsgTelefone">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:maxLength">35</item>
    </style>

    <style name="ValorTotal">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Styleinfos">
        <item name="android:textColor">@color/white</item>
    </style>
</resources>